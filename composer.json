{
    "name": "javer/influxdb-odm",
    "type": "library",
    "description": "Provides a PHP object mapping functionality for InfluxDB",
    "keywords": ["influxdb", "doctrine", "odm"],
    "homepage": "https://github.com/javer/influxdb-odm",
    "license": "MIT",
    "authors": [
        {
            "name": "Vadim Borodavko",
            "email": "vadim.borodavko@gmail.com",
            "homepage": "https://github.com/javer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "doctrine/annotations": "^1.12 || ^2.0",
        "doctrine/instantiator": "^1.3.1 || ^2.0",
        "doctrine/persistence": "^3.0",
        "influxdb/influxdb-php": "^1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.7",
        "squizlabs/php_codesniffer": "3.7.*",
        "swivl/php-coding-standard": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Javer\\InfluxDB\\ODM\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4.x-dev"
        }
    }
}
